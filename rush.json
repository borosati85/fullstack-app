{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.150.0",
  "pnpmVersion": "8.15.8",
  "nodeSupportedVersionRange": ">=18.20.3 <19.0.0 || >=20.14.0 <21.0.0",
  "repository": {
    "url": "https://github.com/borosati85/fullstack-app",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": [],
    "preRushx": [],
    "postRushx": []
  },
   "variants": [],
  "telemetryEnabled": false,
  "hotfixChangeEnabled": false,
  "allowedProjectTags": [ "tools", "frontend", "backend", "lib" ],
  "projects": [
    {
      "packageName": "@borosati85/frontend",
      "projectFolder": "apps/frontend",
      "reviewCategory": "production",
      "tags": ["frontend"]
    },
    {
      "packageName": "@borosati85/backend",
      "projectFolder": "apps/backend",
      "reviewCategory": "production",
      "tags": ["backend"]
    },
    {
      "packageName": "@borosati85/shared",
      "projectFolder": "packages/shared",
      "reviewCategory": "library",
      "tags": ["lib"]
    }
  ]
}
